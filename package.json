{
  "name": "mug-math",
  "version": "3.0.0",
  "description": "A progressive web app that contains several \"mini apps\" of various coffee-related tools.",
  "main": "index.js",
  "repository": "git@github.com:musicbender/mug-math.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "dev": "export NODE_ENV=development; webpack-dev-server --hot --progress --host 0.0.0.0 --port 8087 --config webpack.dev.config.js",
    "bs": "export NODE_ENV=production; webpack --config webpack.server.config.js",
    "bc": "export NODE_ENV=production; webpack --config webpack.prod.config.js",
    "build": "rimraf dist && concurrently \"npm run bc\" \"npm run bs\"  \"nodemon index.js\"",
    "start": "node index.js"
  },
  "keywords": [],
  "author": "Pat Jacobs",
  "license": "ISC",
  "engines": {
    "node": "7.0.0"
  },
