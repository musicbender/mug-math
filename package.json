{
  "name": "mug-math",
  "version": "1.0.0",
  "description": "A progressive web app that contains several \"mini apps\" of various coffee-related tools.",
  "main": "index.js",
  "repository": "git@github.com:musicbender/mug-math.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "start": "export NODE_ENV=development; webpack-dev-server --env=dev --hot --https --progress --host 0.0.0.0 --config webpack.dev.config.js",
    "bs": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.server.config.js",
    "bc": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.prod.config.js",
    "build": "npm run clean && npm run bc && npm run bs",
    "open:dist": "export NODE_ENV=production; export LIVE=false; export PORT=3001; nodemon index.js",
    "build:open": "npm run build && npm run open:dist",
    "start:live": "export PORT=49152; export HTTP_PORT=49153; export NODE_ENV=production; forever -w --watchDirectory ./dist start index.js",
    "deploy:roots": "babel-node tools/deploy-roots.js",
    "deploy:www": "babel-node tools/deploy-www.js",
    "deploy": "export NODE_ENV=production; export LIVE=true; export PORT=49152; export HTTP_PORT=49153; npm run clean && babel-node tools/deploy.js && npm run deploy:roots"
  },
  "keywords": [],
  "author": "Pat Jacobs",
  "license": "ISC",
  "engines": {
    "node": "7.0.0"
  },
  "dependencies": {
    "array.prototype.move": "0.0.4",
    "body-parser": "^1.18.2",
    "express": "^4.15.2",
    "express-force-ssl": "^0.3.2",
    "history": "^4.6.1",
    "material-ui": "^0.19.0",
    "pug": "^0.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^2.1.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-css-modules": "^2.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.3.0",
    "null-loader": "^0.1.1",
    "offline-plugin": "^4.7.0",
    "pug-loader": "^2.3.0",
    "react-addons-perf": "^15.4.2",
    "rsync": "^0.6.1",
    "sass-loader": "^6.0.3",
    "sass-material-colors": "0.0.5",
    "serviceworker-webpack-plugin": "^0.2.1",
    "source-map-support": "^0.4.14",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-node-externals": "^1.6.0"
  }
}
